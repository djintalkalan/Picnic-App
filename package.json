{
  "name": "picnic-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean-install": "rm -rf node_modules; rm -rf yarn.lock; rm -rf ios/Pods; rm -rf ios/Podfile.lock; yarn",
    "backup": "git archive --format zip --output ./backup.zip master",
    "clean:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && npm cache clean --force && yarn cache clean",
    "pod": "npx pod-install",
    "pod-clean": "cd ios && rm -rf Podfile.lock && rm -rf Pods && pod install --repo-update && cd ..",
    "reset": "npx react-native start --reset-cache",
    "release": "cd android && ./gradlew assembleRelease && cd .. && run-s -c open-android-folder open-diawi",
    "bundle": "cd android && ./gradlew bundleRelease && cd .. && run-s -c open-bundle-folder open-play-console",
    "clean": "cd android && ./gradlew clean && cd ..",
    "env-p": "yarn rnuc production.yaml",
    "env-d": "yarn rnuc dev.yaml",
    "env-b": "yarn rnuc beta.yaml",
    "apk-p": "run-s -c kill-all env-p clean release",
    "apk-d": "run-s -c kill-all env-d clean release",
    "apk-b": "run-s -c kill-all env-b clean release",
    "bundle-d": "run-s -c kill-all env-d clean bundle",
    "bundle-p": "run-s -c kill-all env-p clean bundle",
    "bundle-b": "run-s -c kill-all env-b clean bundle",
    "tcp": "adb reverse tcp:9090 tcp:9090",
    "fix-sdk": "(cd ~/Library/Android/sdk/build-tools/31.0.0 && mv d8 dx && cd lib && mv d8.jar dx.jar)",
    "postinstall": "(node ./scripts/fix-carousel.js||true) && run-s env-d pod",
    "kill-all": "((pkill -f '.*GradleDaemon.*' || ./android/gradlew gradle --stop) && (rm -rf ~/.gradle/daemon || true))",
    "dl": "/usr/bin/xcrun simctl openurl booted ",
    "push-a-d": "yarn env-d && appcenter codepush release-react -a picnicshine/picnic-groups-android -d dev -m --target-binary-version",
    "push-a-b": "yarn env-b && appcenter codepush release-react -a picnicshine/picnic-groups-android -d beta -m --target-binary-version",
    "push-a-p": "yarn env-p && appcenter codepush release-react -a picnicshine/picnic-groups-android -d production -m --target-binary-version",
    "push-i-d": "yarn env-d && appcenter codepush release-react -a picnicshine/picnic-groups-ios -d dev -m --target-binary-version",
    "push-i-b": "yarn env-b && appcenter codepush release-react -a picnicshine/picnic-groups-ios -d beta -m --target-binary-version",
    "push-i-p": "yarn env-p && appcenter codepush release-react -a picnicshine/picnic-groups-ios -d production  -m --target-binary-version",
    "ipa-d": "run-s env-d clean-ios archive",
    "ipa-b": "run-s env-b clean-ios archive",
    "ipa-p": "run-s env-p clean-ios archive",
    "diawi-d": "run-s env-d clean-ios archive-diawi",
    "diawi-b": "run-s env-b clean-ios archive-diawi",
    "diawi-p": "run-s env-p clean-ios archive-diawi",
    "archive": "run-s generate-archive generate-ipa generate-ipa-diawi",
    "archive-diawi": "run-s generate-archive generate-ipa-diawi",
    "open-ios-folder": "open ./ios/output",
    "open-android-folder": "open ./android/output",
    "open-bundle-folder": "open ./android/app/build/outputs/bundle/release",
    "open-diawi": "open -a 'Google Chrome' 'https://www.diawi.com/' --args '--new-window'",
    "open-play-console": "open -a 'Google Chrome' 'https://play.google.com/console/u/0/' --args '--new-window'",
    "open-transporter": "(pkill -x Transporter ||true) && open -n -F -a Transporter --args $PWD'/ios/output/Picnic.ipa'",
    "clean-ios": "xcodebuild clean -workspace ios/Picnic.xcworkspace -scheme Picnic && xcodebuild clean -project ios/Picnic.xcodeproj -scheme Picnic && rm -rf ios/build && rm -rf ios/output",
    "generate-archive": "xcodebuild -sdk iphoneos -workspace ios/Picnic.xcworkspace -scheme Picnic -configuration Release -archivePath ios/build/Picnic.xcarchive archive",
    "generate-ipa": "xcodebuild -exportArchive -archivePath ios/build/Picnic.xcarchive -exportOptionsPlist ios/Picnic/exportOptions.plist -exportPath ios/output -allowProvisioningUpdates && yarn open-transporter",
    "generate-ipa-diawi": "xcodebuild -exportArchive -archivePath ios/build/Picnic.xcarchive -exportOptionsPlist ios/Picnic/exportOptionsDiawi.plist -exportPath ios/output/diawi -allowProvisioningUpdates && yarn open-ios-folder"
  },
