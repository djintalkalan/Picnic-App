{
  "name": "rnstarter",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "npx react-native start",
    "test": "jest",
    "lint": "eslint .",
    "backup": "git archive --format zip --output ./backup.zip master",
    "clean:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && npm cache clean --force && yarn cache clean",
    "pod": "npx pod-install",
    "reset": "npx react-native start --reset-cache",
    "release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "clean": "cd android && ./gradlew clean && cd ..",
    "env-p": "yarn rnuc production.env",
    "env-s": "yarn rnuc staging.env",
    "apk-p": "yarn env-p && yarn release",
    "apk-s": "yarn env-s && yarn release",
    "tcp": "adb reverse tcp:9090 tcp:9090",
    "resetIos": "yarn env-s && cd ios && pod deintegrate && rm -rf Podfile.lock && rm -rf Pods && pod install && cd .. && yarn run ios",
    "resetAndroid": "yarn env-s && cd android && ./gradlew clean && cd .. && yarn run android",
    "postinstall": "yarn env-s && npx pod-install"
  },
  "dependencies": {
    "@react-native-community/blur": "^3.6.0",
    "@react-native-community/clipboard": "^1.5.1",
    "@react-native-community/datetimepicker": "^3.5.2",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/material-top-tabs": "^6.0.6",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "@twotalltotems/react-native-otp-input": "^1.3.11",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "dj-intl-phone-input": "^1.0.2",
    "dj-react-native-dropdown-alert": "^0.0.3",
    "react": "17.0.2",
    "react-hook-form": "^7.17.5",
    "react-native": "0.65.1",
    "react-native-bootsplash": "^3.2.5",
    "react-native-cardview": "^2.0.5",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-crop-picker": "^0.37.2",
    "react-native-indicators": "^0.17.0",
    "react-native-internet-connection-alert": "^0.1.6",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-loading-spinner-overlay": "^2.0.0",
    "react-native-localization": "^2.1.7",
    "react-native-material-ripple": "^0.9.1",
    "react-native-mmkv-storage": "^0.6.6",
    "react-native-modal-datetime-picker": "^13.0.0",
    "react-native-pager-view": "^5.4.9",
    "react-native-reanimated": "^2.2.3",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.8.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-swipe-list-view": "^3.2.9",
    "react-native-tab-view": "^3.1.1",
    "react-native-ultimate-config": "^3.3.4",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.16",
    "@types/react-native": "^0.64.5",
    "@types/react-native-indicators": "^0.16.1",
    "@types/react-native-material-ripple": "^0.9.2",
    "@types/react-native-vector-icons": "^6.4.8",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^26.6.3",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.0",
    "react-native-codegen": "^0.0.7",
    "react-test-renderer": "17.0.2",
    "reactotron-react-native": "^5.0.0",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
